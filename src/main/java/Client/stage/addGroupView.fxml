<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="320.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.Client.chatroom.addGroupController">
  <columnConstraints>
    <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints maxHeight="60.0" minHeight="2.0" prefHeight="60.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="110.0" minHeight="10.0" prefHeight="55.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="170.0" minHeight="10.0" prefHeight="149.0" valignment="TOP" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="66.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="25.0" minHeight="0.0" prefHeight="6.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="群聊名称" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </GridPane.margin>
      </Label>
      <Label text="邀请成员" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
      </Label>
      <TextField fx:id="groupNameTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <ListView fx:id="groupChatMenberListView" prefHeight="33.0" prefWidth="153.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button fx:id="ensureButton" mnemonicParsing="false" onAction="#createGroup" prefHeight="72.0" prefWidth="60.0" text="确定" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
      </Button>
      <Label text="创建群聊" GridPane.columnIndex="1" />
   </children>
</GridPane>
